{
   "background": {
      "persistent": true,
      "scripts": [ "javascripts/tabWatcher.js" ]
   },
   "default_locale": "en",
   "description": "Well behaved browser tabs for Zendesk agents",
   "icons": {
      "128": "images/icons/icon-128.png",
      "16": "images/icons/icon-16.png",
      "19": "images/icons/icon-19.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Zendesk Quicktab",
   "page_action": {
      "default_icon": {
         "19": "images/icons/icon38-enabled.png",
         "38": "images/icons/icon38-enabled.png"
      },
      "default_popup": "popup.html",
      "default_title": "Zendesk Quicktab"
   },
   "permissions": [ "http://*.zendesk.com/*", "https://*.zendesk.com/*", "webNavigation", "tabs" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.11.6",
   "applications": {
     "gecko": {
       "id": "{fcfc5d98-01a9-4d8e-8816-2ba9506904af}"
     }
   }
}
